{
    "containerDefinitions": [
        {
            "name": "nodejsapp-container",
            "image": "700740239256.dkr.ecr.ap-south-1.amazonaws.com/nodejsapp:latest",
            "cpu": 0,
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp"
                }
            ],
            "essential": true,
            "environment": [],
            "environmentFiles": [],
            "mountPoints": [],
            "volumesFrom": [],
            "ulimits": [],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-create-group": "true",
                    "awslogs-group": "/ecs/nodejsapp-td",
                    "awslogs-region": "ap-south-1",
                    "awslogs-stream-prefix": "ecs"
                }
            }
        }
    ],
    "family": "nodejsapp-td",
    "taskRoleArn": "arn:aws:iam::700740239256:role/ecsTaskExecutionRole",
    "executionRoleArn": "arn:aws:iam::700740239256:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "volumes": [],
    "placementConstraints": [],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "1024",
    "memory": "3072",
    "tags": []
}
